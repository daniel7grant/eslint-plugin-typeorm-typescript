    "author": "Daniel Grant",
    "license": "ISC",
    "devDependencies": {
        "@typescript-eslint/rule-tester": "^8.0.0-alpha.16",
        "eslint": "^9.3.0",
        "prettier": "^3.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.3",
        "typescript-eslint": "^8.0.0-alpha.16",
        "vitest": "^1.1.0"
    },
    "dependencies": {
        "@typescript-eslint/parser": "^8.0.0-alpha.16",
        "@typescript-eslint/utils": "^8.0.0-alpha.16",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3"
